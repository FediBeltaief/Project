<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project.AjouterCategorie">
    <ContentPage.Content>
        <StackLayout>
            <TableView>
                <TableRoot>
                    <TableSection>
                        <EntryCell x:Name="txtId" Label="Id" IsEnabled="False"/>
                        <EntryCell x:Name="txtNom" Label="Name"/>
                    </TableSection>
                </TableRoot>
            </TableView>
            <Button x:Name="btnadd" Clicked="Btnadd_Clicked" Text="Save" BackgroundColor="#001F3F" TextColor="White"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>